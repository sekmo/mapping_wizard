<p id="notice"><%= notice %></p>

<h1>Mappings</h1>

<table class="mapping-index">
  <thead>
    <tr>
      <th>Partner</th>
      <th>Dalia key</th>
      <th>Dalia value</th>
      <th>Partner key</th>
      <th>Partner value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mappings.each do |mapping| %>
      <tr>
        <td><%= mapping.partner %></td>
        <td><%= mapping.dalia_key %></td>
        <td><%= mapping.dalia_value %></td>
        <td><%= mapping.partner_key %></td>
        <td><%= mapping.partner_value %></td>
        <td><%= link_to 'Show', mapping %></td>
        <td><%= link_to 'Edit', edit_mapping_path(mapping) %></td>
        <td><%= link_to 'Destroy', mapping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mapping', new_mapping_path %>
